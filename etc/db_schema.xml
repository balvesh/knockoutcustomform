<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="learning_customform_customform" resource="default" engine="innodb" comment="learning_customform_customform Table">
		<column xsi:type="smallint" name="customform_id" padding="6" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="customform_id"/>
		</constraint>
		<column name="name" nullable="true" xsi:type="varchar" comment="name" length="255"/>
		<column name="email" nullable="true" xsi:type="varchar" comment="email" length="255"/>
		<column name="phone" nullable="true" xsi:type="varchar" comment="phone" length="255"/>
		<column name="message" nullable="true" xsi:type="text" comment="message"/>
		<column name="created_at" nullable="true" xsi:type="timestamp" comment="created_at"/>
	</table>
</schema>
